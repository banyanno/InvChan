                <SelectCommand>
                  <DbCommand CommandType="Text" ModifiedByUser="true">
                    <CommandText>SELECT P.DETAIL_ID,

(SELECT CUST_NAME FROM INVOICE AS I WHERE I.INVOICE_ID=P.INVOICE_ID) AS CUST_NAME,

(SELECT CUST_ADDRESS FROM CUSTOMER AS C WHERE C.CUST_ID=(SELECT CUST_ID FROM INVOICE AS I WHERE I.INVOICE_ID=P.INVOICE_ID)) AS CUST_ADDRESS,

(SELECT CUST_PHONE FROM CUSTOMER AS C WHERE C.CUST_ID=(SELECT CUST_ID FROM INVOICE AS I WHERE I.INVOICE_ID=P.INVOICE_ID)) AS CUST_PHONE,

P.INVOICE_ID,P.ITEM_ID,

(SELECT ITEM_NAME FROM ITEMS AS I WHERE I.ITEM_ID=P.ITEM_ID) AS ITEM_NAME, 

P.UNIT_NAME, P.QTY,

(CONVERT(varchar(25),P.QTY)+''+P.UNIT_NAME) AS ITEM_QTY,

(SELECT PRICE FROM INVOICE_DETAILS AS D WHERE D.DETAIL_ID=P.DETAIL_ID AND D.CURR_CODE='USD') AS USD_PRICE,
(SELECT PRICE FROM INVOICE_DETAILS AS D WHERE D.DETAIL_ID=P.DETAIL_ID AND D.CURR_CODE='KHR') AS KHR_PRICE,
(SELECT PRICE FROM INVOICE_DETAILS AS D WHERE D.DETAIL_ID=P.DETAIL_ID AND D.CURR_CODE='THB') AS THB_PRICE,

(CONVERT(varchar(25),P.PRICE)+''+P.CURR_CODE) AS ITEM_PRICE,

(CONVERT(varchar(25),P.DISCOUNT)+' '+P.CURR_CODE) AS ITEM_DISCOUNT,


P.PRICE,P.DISCOUNT,P.CURR_CODE,P.KHR_AMOUNT, P.USD_AMOUNT, P.THB_AMOUNT,

(SELECT INVOICE_DATE FROM INVOICE AS I WHERE I.INVOICE_ID=P.INVOICE_ID) AS INVOICE_DATE,
(SELECT INVOICE_NO FROM INVOICE AS I WHERE I.INVOICE_ID=P.INVOICE_ID) AS INVOICE_NO,P.QTY_FREE,P.INIT_NAME_FREE,
(CONVERT(varchar(25),P.QTY_FREE)+''+P.INIT_NAME_FREE) AS ITEM_QTY_Free,P.IS_EXTRA
FROM dbo.INVOICE_DETAILS  AS P

WHERE P.INVOICE_ID=@InvoiceID
